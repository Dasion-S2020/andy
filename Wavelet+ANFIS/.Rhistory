a <- c(0, 1)
rep.row<-function(x,n){
matrix(rep(x,each=n),nrow=n)
}
rep.col<-function(x,n){
matrix(rep(x,each=n), ncol=n, byrow=TRUE)
}
rep.col(a, 13)
RD <- rep.col(a, 13)
objectNL <- frbs.learn(train, RD, "ANFIS", ControlNL)
pred <- predict(objectNL, testx)
